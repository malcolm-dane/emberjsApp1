

<div class="container">
  
<h3>Photos</h3>
  <div class="row">
    <div class="col-*-*"></div>
  </div>
  <div class="row">
    <div class="col-*-*"></div>
    <ul class='photos-list'>
{{#each model as |photo|}}
	<li>
		<p><img src='{{ photo.thumbnailUrl }}'></p>
		{{ photo.title }}
	</li>
{{/each}}
</ul>
  </div>
</div>
